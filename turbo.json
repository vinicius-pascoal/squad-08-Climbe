{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".env"
  ],
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "lint": {},
    "format": {},
    "prisma:generate": {
      "outputs": []
    },
    "prisma:migrate": {
      "outputs": []
    },
    "prisma:push": {
      "outputs": []
    },
    "prisma:seed": {
      "outputs": []
    }
  }
}
